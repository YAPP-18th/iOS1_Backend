<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.yapp.ios1.mapper.BucketMapper">
    <select id="findByBucketState" resultType="BookDto">
        SELECT * FROM bucket_list WHERE bucket_state = #{bucketState} and user_id = #{userId};
    </select>

    <select id="findByUserBucketListAll" resultType="BookDto">
        SELECT * FROM bucket_list WHERE user_id = #{userId};
    </select>
</mapper>